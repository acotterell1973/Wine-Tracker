<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WineTracker.Pages.ScanProductPage">
  <StackLayout>
    <Entry Text="{Binding Model.number}" Placeholder="Number" ></Entry>

    <Button Text="Look Up" Command="{Binding SiginCommand}" ></Button>
    <Button Text="Scan" Command="{Binding ScanbarCode}" ></Button>
  </StackLayout>

</ContentPage>